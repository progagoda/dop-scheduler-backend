{
    "version": 2,
    "buildCommand": "npm run build", 
    "builds": [
        {
            
            "src": "src/main",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "OPTIONS",
                "DELETE"
            ]
        }
    ]
}