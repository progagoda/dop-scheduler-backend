{
    "version": 2,
    "builds": [
        {
            "buildCommand": "npm run build", 
            "src": "src/main.ts",
            "use": "@vercel/node",
            "framework": "nestjs"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "OPTIONS",
                "DELETE"
            ]
        }
    ]
}