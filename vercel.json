{
    "version": 2,
    "buildCommand": "npm run build",
    "env": {
        "SECRET_KEY": "@secret_key",
        "PORT": "@port",
        "POSTGRES_PRISMA_URL": "@postgres_prisma_url",
        "POSTGRES_URL_NON_POOLING": "@postgres_url_non_pooling"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "OPTIONS",
                "DELETE"
            ]
        }
    ]
}